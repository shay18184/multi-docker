{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image":"shayliboironsrc/multi-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image":"shayliboironsrc/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [{
        "hostPort": 80,
        "containerPort": 80
      }],
      "links": ["client"],
      "memory": 128
    }
  ]
}